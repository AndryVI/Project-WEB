@{
    ViewData["Title"] = "Day";
    Layout = "_LayoutMenu";
}

<div class="container marketing " style="padding: 0 0 0 20px">
    <div class="Mypadding">
        <h2>Дни</h2>
        <table>
            @foreach (var item in @ViewBag.Day)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                </tr>
            }
        </table>

    </div>

</div>
